//import java.util.LinkedList;
import java.util.*;

public class QueueExample {
    public static void main(String[] args) {
        // Create and initialize a Queue using a LinkedList
        Queue<String> waitingQueue = new LinkedList<String>();

        // Adding new elements to the Queue (The Enqueue operation)
        waitingQueue.add("dogcat");
        //waitingQueue.add("dog");
        waitingQueue.add("cat");
        waitingQueue.add("mouse");
        waitingQueue.add("cat");
        //System.out.println(waitingQueue);
        waitingQueue.add("dog");
        System.out.print(waitingQueue.remove());
        waitingQueue.remove();
        //System.out.println(waitingQueue);
        waitingQueue.add("cat");
        System.out.print(waitingQueue.remove());
        System.out.print(waitingQueue.remove());
        System.out.println(waitingQueue.remove());

        //waitingQueue.add("Mark");
        //waitingQueue.add("Steven");
        //if (waitingQueue.isEmpty()) {
          //waitingQueue.add("a");
          //waitingQueue.add("b");
          //waitingQueue.add("c");
        //}

        //System.out.println("WaitingQueue : " + waitingQueue);

        // Removing an element from the Queue using remove() (The Dequeue operation)
        // The remove() method throws NoSuchElementException if the Queue is empty
        //String name = waitingQueue.remove();
        //System.out.println("Removed from WaitingQueue : " + name + " | New WaitingQueue : " + waitingQueue);
      }
}
